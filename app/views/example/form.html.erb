
<%= form_tag("/job_applications", method: "post",  id: "new_job_application") do %>
  <p>
    <%= label_tag(:full_name, "Full Name") %>
    <%= text_field_tag(:full_name) %>
  </p>
  <p>
    <%= label_tag(:phone, "Phone")%>
    <%= text_field_tag(:phone)%>
  </p>
  <p>
    <%= label_tag(:hobby, "Hobby")%>
    <%= text_field_tag(:hobby)%>
  </p>
  <p>
    <%= label_tag(:years_exp, "Years Experience")%>
    <%= text_field_tag(:years_exp)%>
  </p>
  <p>
    <%= label_tag(:available_date, "Available Date")%>
    <%= text_field_tag(:available_date)%>
  </p>
  <p>
    <%= submit_tag("Create Job Application")%>
  </p>
<% end %>
